{
  "expo": {
    "name": "Food-Delivery-App",
    "slug": "food-delivery-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "packagerOpts": {
      "sourceExts": [
        "cjs"
      ]
    },
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Food Delivery App Dashboard",
    "githubUrl": "https://github.com/kevinrodriguez-io/food-delivery-app"
  }
}
